# Environment
env=dev

# GCP / BigQuery
gcp_project=
bq_default_location=asia-northeast3
maximum_bytes_billed=5000000000
price_per_tb_usd=5.0
dry_run_only=true

# BigQuery table overrides (optional)
bq_events_table=`proj.mall.events`
bq_users_table=`proj.mall.users`
bq_orders_table=`proj.mall.orders`

# Optional: service account JSON path for local runs
# GOOGLE_APPLICATION_CREDENTIALS=C:\path\to\service-account.json

# LLM Provider (openai | claude | gemini)
llm_provider=openai

# OpenAI
openai_api_key=
openai_model=gpt-4o-mini

# Anthropic (Claude)
anthropic_api_key=
anthropic_model=claude-3-5-sonnet-20240620

# Google Gemini
gemini_api_key=
gemini_model=gemini-1.5-flash
